
const puts = @extern("puts", proc(*u8) -> i32);

const print_number = proc(n: i32) {
    const printf_hack = @extern("printf", proc(*u8, i32) -> i32);
    printf_hack("%d".data, n);
    puts("".data);
};

const main = proc() -> i32 {
    var i = 0;
    while i < 100 {
        if i % 3 == 0 && i % 5 == 0 {
            puts("FizzBuzz".data);
        }
        else if i % 3 == 0 {
            puts("Fizz".data);
        }
        else if i % 5 == 0 {
            puts("Buzz".data);
        }
        else {
            print_number(i);
        }
        i += 1;
    }
    0
};
