
const main = proc() -> i32 {
    var i = 2016;
    while i > 0 {
        if no_divisors(i) == 8 && contains_7(i) {
            return 2020 - i;
        }
        i = i - 1;
    }
    0
};

const contains_7 = proc(n: i32) -> bool {
    while n > 0 {
        if n % 10 == 7 {
            return true;
        }
        n = n / 10;
    }
    false
};

const no_divisors = proc(n: i32) -> i32 {
    var i = 1;
    var cnt = 0;
    while i <= n {
        if n % i == 0 {
            cnt = cnt + 1;
        }
        i = i + 1;
    }
    cnt
};
