
const get_type = proc(x: type) -> type { x }

const some_type: get_type(type) = get_type(i32);

const some_c_fun = @extern("hello", i32);
const c_method = @extern("bye", proc(i32) -> i32);

const foo = proc(x: i32) -> i32 { x }

const bar = proc() {}

const rec = proc(x: i32) -> i32 { rec(x) }

const Person = struct {
	name: i32;
	age : i32;
}

const List = proc(T: type) -> type {
	struct {
		data  : T  ;
		length: i32;
	}
}

const main = proc() -> get_type(i32) {
	bar();
	foo(2);
	c_method(56);
	some_c_fun
}
